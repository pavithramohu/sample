salary=int(input("salary="))
if salary<5000:
    x=salary*0.5 
    y=x+salary
    print(y)
elif salary>5000 and salary<10000:
    x=salary*1.0
    y=x+salary
    print(y)
elif salary>10000 and salary<15000:
    x=salary*1.5 
    y=x+salary
    print(y)

