------------------------------------------------------------
def cigar_party(weekend,n):
    if (not weekend) and (n>=40 and n<=60) :
        return True
    elif weekend and n>40:
        return True
    else:
        return False
res=cigar_party(False,50)
print(res)

O/P
True
-------------------------------------------------------------

def caught_speeding(speed,bday):
    if speed<=60 and not bday:
        return 0
    elif speed>=65  and  bday:
        return 0 
    if (speed >=61 and speed<=80) and not bday:
        return 1 
    elif (speed >=65 and speed <=85) and bday :
        return 1
    else:
        return 2
res=caught_speeding(60,False)
print(res)

