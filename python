l=["tamil","english","maths","science","social","biology"]
start=int(input("enter starting index"))
end=int(input("enter ending index"))
new_list=[]
for i in range (0,3):
    get= input()
    new_list.append(get)
print(new_list)
for i in range (0,3):
    l[start]=new_list[i]
    start=start+1
print(l)
        
